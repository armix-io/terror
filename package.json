{
  "name": "@armix/terror",
  "version": "1.0.0",
  "description": "Universally better error-handling for TypeScript as a tiny, zero-dependency general purpose utility.",
  "main": "index.js",
  "repository": "git@github.com:ptboyer/terror.git",
  "author": "Peter Boyer <github.com/ptboyer>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "dev": "yarn test && yarn test:ts && yarn lint",
    "test": "jest",
    "test:ts": "tsc --noEmit",
    "test:unit": "./scripts/test-unit.sh",
    "test:acceptance": "./scripts/test-acceptance.sh",
    "lint": "eslint src",
    "build": "./scripts/build.sh",
    "build:version": "./scripts/build-version.sh",
    "build:tag": "./scripts/build-tag.sh",
    "docs": "typedoc ./src/index.ts --excludeInternal --disableSources",
    "changelog": "./scripts/changelog.sh"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "conventional-changelog-cli": "^2.1.1",
    "dot-json": "^1.2.2",
    "esbuild": "^0.13.12",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "tsd": "^0.18.0",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4"
  },
  "keywords": [
    "typed",
    "error",
    "handling",
    "throw",
    "result",
    "typescript"
  ]
}
